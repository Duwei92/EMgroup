<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="EMgroup.admin.dto.AdminInfo" table="admin">
      <id name="id" type="java.lang.String" column="id">
         <generator class="UUID"/>
      </id>
      <property name="username" column="username" type="passward"/>
      <property name="passward" column="passward" type="passward"/>
      <property name="logtime" column="logtime" type="java.util.Date"/>
   </class>
</hibernate-mapping>